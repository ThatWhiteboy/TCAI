{
  "name": "titan-cloud-ai",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "npm-run-all --parallel type-check lint test",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "predeploy": "npm run validate",
    "deploy": "npm run build",
    "postdeploy": "node scripts/verify-deployment.js"
  },
  "dependencies": {
    "@elastic/apm-rum": "^5.15.0",
    "@netlify/functions": "^2.6.0",
    "@sentry/node": "^7.101.1",
    "@stripe/stripe-js": "^2.4.0",
    "@tanstack/react-query": "^5.17.19",
    "@trpc/client": "^11.0.0-next-beta.294",
    "@trpc/react-query": "^11.0.0-next-beta.294",
    "@trpc/server": "^11.0.0-next-beta.294",
    "chart.js": "^4.4.1",
    "compression": "^1.7.4",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.3.1",
    "dd-trace": "^5.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "lucide-react": "^0.344.0",
    "newrelic": "^11.10.3",
    "node-cron": "^3.0.3",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "systeminformation": "^5.21.22",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.0.7",
    "autoprefixer": "^10.4.18",
    "cssnano": "^6.0.5",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "memfs": "^4.7.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.28.1",
    "typescript": "^5.3.3",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^3.0.7",
    "vitest-fetch-mock": "^0.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
